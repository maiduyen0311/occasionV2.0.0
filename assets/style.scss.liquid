.search-page{
  .search-product-ct{
      border: 1px solid #d7d7d7;

      .products-grid{
          margin:0;
   		  margin-bottom: -1px;
        
          .page-search{
              padding-top:20px;             
              border-bottom: 1px solid #d7d7d7;
            	
              &:before {
                content: "";
                border-right: 1px solid #d7d7d7;
                position: absolute;
                right: -2px;
                bottom: 0;
                top: 0;
              }
          }
      }
  }
  
  .padding{
    width:100%;
    padding:20px 15px;
    
    .toolbar-bottom{
      @include flex(center, space-between);
      
      .page-total{
      	margin-bottom:0;
      }
      
      ul{
        padding: 0px;
        list-style: none;
        margin: 0px;
        
        li{
          display: inline-block;
          padding-right: 5px;
          
          a{
            border: 1px solid #d7d7d7;
            padding: 10px 14px;
            line-height: normal;
            border-radius: 3px;
            color: #000;
            text-decoration: none;
          }
        }
        
        .disabled{
          border: 1px solid #d7d7d7;
          padding: 10px 14px;
          line-height: normal;
          border-radius: 3px;
          color: #000;
          text-decoration: none;
          margin-right: 5px;
        }
        
        .active span {
          border: 1px solid #fafafa;
          padding: 10px 14px;
          line-height: normal;
          border-radius: 3px;
          color: #000;
          text-decoration: none;
          font-weight: 700;
          background: #fafafa;
        }
      }
    }
  }
}

.currencies_modal{
    position: absolute;
    right: 15px;
    background: #dff0d8;
    padding: 2px 15px;
    border-radius: 3px;
    z-index: 200;
    color: #3c763d;
    letter-spacing: 0.05em;
    
    div{
        display: inline-block;
        margin-right: 5px;
    }
    
    span{
        font-weight: 700;
        color: red;
    }
}


.btn{
  overflow: hidden;
  text-overflow: ellipsis;
  font-family: $font_3;
}

.form-success {
    background-color: #dff0d8;
    border-color: #d6e9c6;
    color: #3c763d;
    padding: 10px 20px;
    border-radius: 4px;
    letter-spacing: 0.02em;
    font-size: 12px;
}

.resetpass_page{
    label {
        display: block;
        font-weight: 700;
    }
    
    input{
        margin-bottom: 20px;
        max-width: 420px;
    }
    
    .btn-action{
        .btn{
            text-transform: uppercase;
            color: #fff;
            font-weight: 700;
            letter-spacing: 0.02em;
            text-align: center;
            min-width: 160px;
            cursor: pointer;
            font-size: 12px;
          	@include button_3;
          	
          	&:hover{
            	@include button_2;
            }
        }
    }
}

.ico-mb-first{
	display:none!important;
}
.overflow-hidden {
    position: fixed;
    overflow: hidden;
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}
.refined-widgets{
    margin-top: 20px;
    margin-bottom: 30px;
    
    .widget-title{
        h3{
            padding:13px 0;
        }
    }
    ul{
        margin-bottom: 15px;
        list-style: none;
        padding-top: 15px;
        
        li{
            display: inline-block;
            margin: 0 5px 12px 0;
            input{
                display: none;
            }
            
            a{
                background: #a4a4a4;
                color: #fff;
                padding: 5px 10px;
                border-radius: 3px;
                text-decoration: none;
                
                &:hover{
                    background: #333;
                    color:#fff;
                }
                
                .clear-tags{
                    font-weight: 700;
                    padding-left: 5px;
                }
            }
        }
    }
    .clear-all{
        display: inline-block;
        margin-top: 5px;
        border-bottom: 1px dotted;
        color: #f04723;
        text-decoration: none;
        font-weight: 700;
    }
}
{% if settings.enable_hover_image %}

%hover_img{
  display: block;
  position: relative;
  overflow: hidden;

  &:before{
    content: "";
    background-color: #ffffff24;
    display: block;
    position: absolute;
    left: -100%;
    top: 0;
    width: 100%;
    height: 100%;
    transition: all .3s ease 0s;
    z-index: 1;
  }
  
  &:hover:before{
    left: 0;
  }
}

.home-slideshow{
  .column-right{
    a{
      @extend %hover_img;
    }
  }
}

.banner-home-top{
  a{
    @extend %hover_img;
  }
}

.collections-homepage{
  a{
    @extend %hover_img;
  }
}

.bottom-block-banner{
  a{
    @extend %hover_img;
  }
}

.banner-one-block{
  a{
    @extend %hover_img;
  }
}
{% endif %}

