        position: relative;
        width: 20px;
        height: 2px;
        transition: 0.2s;
        border-radius: 3px;
        display: block;
        float: right;
        
        &:before{
            display: block;
            content: "";
            height: 2px;
            width: 20px;
            position: absolute;
            transition: 0.2s;
            border-radius: 3px;
            display: block;
            top:-6px;
        }

        &:after{
            display: block;
            content: "";
            height: 2px;
            width: 20px;
            position: absolute;
            transition: 0.2s;
            border-radius: 3px;
            display: block;
            top:6px;
        }
    }
}

